{
    "compilerOptions": {
        "outDir": "./.dist/",
        "sourceMap": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "lib": [
            "es6",
            "dom"
        ]
    },
    "exclude": [
        "bin",
        "node_modules",
        ".dist"
    ]
}
